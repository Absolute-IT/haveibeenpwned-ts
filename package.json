{
  "name": "haveibeenpwned",
  "version": "1.0.0",
  "description": "Have I Been Pwned API client",
  "main": "dist/index.js",
  "type": "module",
  "scripts": {
    "start": "pnpm build && node dist/index.js",
    "build": "tsc -p .",
    "prepare": "pnpm build",
    "test": "NODE_OPTIONS=--experimental-vm-modules jest",
	"test:live": "ENABLE_LIVE_TESTS=true NODE_OPTIONS=--experimental-vm-modules jest",
    "test:watch": "NODE_OPTIONS=--experimental-vm-modules jest --watch",
    "test:coverage": "NODE_OPTIONS=--experimental-vm-modules jest --coverage"
  },
  "keywords": [
    "haveibeenpwned",
    "api",
    "client",
    "typescript"
  ],
  "author": "Matthew Scott",
  "license": "GPL-3.0-or-later",
  "packageManager": "pnpm@10.5.0",
  "devDependencies": {
    "@jest/globals": "^29.7.0",
    "@types/jest": "^29.5.14",
    "@types/node": "^22.13.5",
    "dotenv": "^16.4.7",
    "eslint": "^9.21.0",
    "eslint-config-love": "^118.0.0",
    "globals": "^16.0.0",
    "jest": "^29.7.0",
    "jest-fetch-mock": "^3.0.3",
    "prettier": "^3.5.2",
    "pretty-quick": "^4.0.0",
    "ts-jest": "^29.2.6",
    "typescript": "^5.7.3",
    "typescript-eslint": "^8.25.0"
  }
